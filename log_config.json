{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)s [%(name)s] %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "default": {
      "formatter": "default",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stderr"
    }
  },
  "root": {
    "handlers": ["default"],
    "level": "INFO"
  },
  "loggers": {
    "uvicorn.access": {
      "level": "WARNING"
    }
  }
}
